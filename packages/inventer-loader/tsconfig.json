{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "downlevelIteration": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitThis": true,
    "outDir": "es"
  },
  "include": ["src/**/*"]
}

